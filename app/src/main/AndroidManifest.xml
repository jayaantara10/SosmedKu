<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="id.jayaantara.sosmedku">

    <application
        android:allowBackup="true"
        android:icon="@drawable/launcher"
        android:label="@string/app_name"
        android:roundIcon="@drawable/launcher"
        android:supportsRtl="true"
        android:theme="@style/Theme.SosmedKu">
        <activity android:name=".SplashScreenActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".controller.ViewDataUserActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".controller.ViewDataSosmedActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".controller.ManajemenUserActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".controller.ManajemenDataSosmedActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".controller.DashboardActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".controller.LoginActivity"
            android:screenOrientation="portrait">

        </activity>
    </application>

</manifest>